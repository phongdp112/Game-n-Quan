<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="homePane" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.controller.HomeController">
      <ImageView fx:id="backgroundImageHome" fitHeight="768.0" fitWidth="1024.0" pickOnBounds="true">
         <image>
            <Image url="@../../image/home_background.jpg" />
         </image>
      </ImageView>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" StackPane.alignment="CENTER">
            <Button fx:id="newGameButton" maxWidth="200.0" mnemonicParsing="false" text="Game mới" />
            <Button fx:id="guideButton" maxWidth="200.0" mnemonicParsing="false" text="Hướng dẫn" />
            <Button fx:id="exitButton" maxWidth="200.0" mnemonicParsing="false" text="Thoát" />
      </VBox>
</StackPane>
